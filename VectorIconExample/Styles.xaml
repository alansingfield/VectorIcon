<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Didsbury.VectorIconExample">

  <!-- A good source of XAML paths is: https://materialdesignicons.com/ -->

  <!-- The material design icons are based on a 24x24 unit grid -->
  <Style x:Key="MaterialDesignIcon" TargetType="local:VectorIcon">
    <Style.Setters>
      <Setter Property="PathHeight" Value="24"/>
      <Setter Property="PathWidth" Value="24"/>
    </Style.Setters>
  </Style>


  <Style x:Key="CopyIcon" 
         TargetType="local:VectorIcon" 
         BasedOn="{StaticResource MaterialDesignIcon}">
    <Style.Setters>
      <Setter Property="Geometry">
        <Setter.Value>
          <PathGeometry Figures="M19,3A2,2 0 0,1 21,5V19A2,2 0 0,1 19,21H5A2,2 0 0,1 3,19V5A2,2 0 0,1 5,3H9.18C9.6,1.84 10.7,1 12,1C13.3,1 14.4,1.84 14.82,3H19M12,3A1,1 0 0,0 11,4A1,1 0 0,0 12,5A1,1 0 0,0 13,4A1,1 0 0,0 12,3M7,7V5H5V19H19V5H17V7H7M16.91,13L11.91,18V15H7.91V11H11.91V8L16.91,13Z"
            />
        </Setter.Value>
      </Setter>
    </Style.Setters>
  </Style>

